{
  "version": "0.1",
  "provider": {
    "name": "apigw",
    "endpoint": "https://apigateway.apigw.ntruss.com/api/v1/api-keys"
  },
  "requests": [
    {
      "create": {
        "request_body": {
          "name": "",
          "required": ["productName", "subscriptionCode"]
        }
      },
      "read": {
        "parameters": ["product-id"]
      },
      "update": [
        {
          "parameters": ["product-id"],
          "request_body": {
            "name": "",
            "required": ["productName", "subscriptionCode"]
          }
        }
      ],
      "delete": {
        "parameters": ["product-id"]
      },
      "name": "product",
      "id": "product.product_id"
    }
  ],
  "resources": [
    {
      "name": "product",
      "schema": {
        "attributes": [
          {
            "name": "description",
            "string": {
              "computed_optional_required": "computed_optional",
              "description": "Description\u003cbr\u003eLength(Min/Max): 0/300"
            }
          },
          {
            "name": "product_name",
            "string": {
              "computed_optional_required": "required",
              "description": "Product Name\u003cbr\u003eLength(Min/Max): 0/100"
            }
          },
          {
            "name": "subscription_code",
            "string": {
              "computed_optional_required": "required",
              "description": "Subscription Code\u003cbr\u003eAllowable values: PROTECTED, PUBLIC",
              "validators": [
                {
                  "custom": {
                    "imports": [
                      {
                        "path": "github.com/hashicorp/terraform-plugin-framework-validators/stringvalidator"
                      }
                    ],
                    "schema_definition": "stringvalidator.OneOf(\n\"PROTECTED\",\n\"PUBLIC\",\n)"
                  }
                }
              ]
            }
          },
          {
            "name": "product",
            "single_nested": {
              "computed_optional_required": "computed",
              "attributes": [
                {
                  "name": "action_name",
                  "string": {
                    "computed_optional_required": "computed",
                    "description": "Action Name"
                  }
                },
                {
                  "name": "disabled",
                  "bool": {
                    "computed_optional_required": "computed",
                    "description": "Disabled"
                  }
                },
                {
                  "name": "domain_code",
                  "string": {
                    "computed_optional_required": "computed",
                    "description": "Domain Code"
                  }
                },
                {
                  "name": "invoke_id",
                  "string": {
                    "computed_optional_required": "computed",
                    "description": "Invoke Id"
                  }
                },
                {
                  "name": "is_deleted",
                  "bool": {
                    "computed_optional_required": "computed",
                    "description": "Is Deleted"
                  }
                },
                {
                  "name": "is_published",
                  "bool": {
                    "computed_optional_required": "computed",
                    "description": "Is Published"
                  }
                },
                {
                  "name": "mod_time",
                  "string": {
                    "computed_optional_required": "computed",
                    "description": "Mod Time"
                  }
                },
                {
                  "name": "modifier",
                  "string": {
                    "computed_optional_required": "computed",
                    "description": "Modifier"
                  }
                },
                {
                  "name": "permission",
                  "string": {
                    "computed_optional_required": "computed",
                    "description": "Permission"
                  }
                },
                {
                  "name": "product_description",
                  "string": {
                    "computed_optional_required": "computed",
                    "description": "Product Description"
                  }
                },
                {
                  "name": "product_id",
                  "string": {
                    "computed_optional_required": "computed",
                    "description": "Product Id"
                  }
                },
                {
                  "name": "product_name",
                  "string": {
                    "computed_optional_required": "computed",
                    "description": "Product Name"
                  }
                },
                {
                  "name": "subscription_code",
                  "string": {
                    "computed_optional_required": "computed",
                    "description": "Subscription Code\u003cbr\u003eAllowable values: PROTECTED, PUBLIC"
                  }
                },
                {
                  "name": "tenant_id",
                  "string": {
                    "computed_optional_required": "computed",
                    "description": "Tenant Id"
                  }
                }
              ]
            }
          },
          {
            "name": "productid",
            "string": {
              "computed_optional_required": "computed_optional",
              "description": "product-id"
            }
          }
        ]
      },
      "dto_name": "PostProductResponse"
    }
  ],
  "datasources": [
    {
      "name": "api_key",
      "schema": {
        "attributes": [
