{{ range $key, $value := . }}
"{{$key}}": schema.MapAttribute{
ElementType: {{ getElementType .ElementType}},
{{- template "common_attribute" $value }}
{{- if gt (len $value.Validators) 0 }}
Validators: []validator.Map{
{{- range $validator := .Validators}}
{{- if .Custom}}
{{.Custom.SchemaDefinition}},
{{- end}}
{{- end}}
},
{{- end}}
},
{{- end }}